<Window x:Class="MasterPolApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MasterPolApplication"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="Мастер пол" Height="650" Width="1000" MinHeight="650" MaxHeight="750" MinWidth="1000" MaxWidth="1100">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Background="#F4E8D3" Grid.Row="0">
            <Label Content="Мастер пол" VerticalAlignment="Center" HorizontalAlignment="Center"
                   FontSize="23" FontWeight="Bold"></Label>
            <Image Source="/res/Мастер пол.png" VerticalAlignment="Center" HorizontalAlignment="Center"
                   Margin="0,0,220,0" Height="65"/>
        </Grid>

        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>

    </Grid>
</Window>
